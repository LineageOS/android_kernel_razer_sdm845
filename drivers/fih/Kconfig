#
# fih driver
#

menu "fih driver"

config FIH
	bool "fih"
	default n
	help
	  support fih


config FIH_RC2
	bool "fih_rc2"
	default n
	help
	  support fih_rc2

config FIH_APR
	bool "fih apr"
	depends on FIH
	default n
	help
	  support fih apr virtual file

