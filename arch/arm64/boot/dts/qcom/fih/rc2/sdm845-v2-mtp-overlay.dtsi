&thermal_zones {
	soc {
		cooling-maps {
			soc_cpu4 {
				cooling-device =
					<&CPU4 19 19>;
			};
			soc_cpu5 {
				cooling-device =
					<&CPU5 19 19>;
			};
			soc_map6 {
				cooling-device =
					<&CPU6 19 19>;
			};
			soc_map7 {
				cooling-device =
					<&CPU7 19 19>;
			};
		};
	};
};
