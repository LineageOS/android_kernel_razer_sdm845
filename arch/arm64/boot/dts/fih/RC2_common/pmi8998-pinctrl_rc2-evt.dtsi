			bias-pull-down;
			power-source = <0>;
		};
	};

	pmi8998_gpio10 { /* PMI_GPIO_10 / TP1509 */
		pmi8998_gpio10_default: pmi8998_gpio10_default {
			pins = "gpio10";
			function = "normal";
			input-enable;
			bias-pull-down;
			power-source = <0>;
		};
	};

	pmi8998_gpio11 { /* PMI_GPIO_11 / TP1505 */
		pmi8998_gpio11_default: pmi8998_gpio11_default {
			pins = "gpio11";
			function = "normal";
			input-enable;
			bias-pull-down;
			power-source = <1>;
		};
		pmi8998_gpio11_sleep: pmi8998_gpio11_sleep {
			pins = "gpio11";
			function = "normal";
			input-enable;
			bias-pull-down;
			power-source = <1>;
		};
	};

	pmi8998_gpio12 { /* PMI_GPIO_12 / TP1506 */
		pmi8998_gpio12_default: pmi8998_gpio12_default {
			pins = "gpio12";
			function = "normal";
			input-enable;
			bias-pull-down;
			power-source = <0>;
		};
		pmi8998_gpio12_sleep: pmi8998_gpio12_sleep {
			pins = "gpio12";
			function = "normal";
			input-enable;
			bias-pull-down;
			power-source = <0>;
		};
	};

	/*pmi8998_gpio13 { // SPMI_I2C_SEL - disallow config
	};*/

	pmi8998_gpio14 { /* PMI_GPIO_14 / TP1507 */
		pmi8998_gpio14_sleep: pmi8998_gpio14_sleep {
			pins = "gpio14";
			function = "normal";
			input-enable;
			bias-pull-down;
			power-source = <0>;
		};
	};
};
